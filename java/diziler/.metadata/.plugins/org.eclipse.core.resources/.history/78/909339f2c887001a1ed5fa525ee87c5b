package cokBoyutlu;

public class cokBoyutlu {

	public static void main(String[] args) {
		
		char arr [][] = new char[5][5];
		for(int i=0; i<5; i++) {
			for(int j=0; j<5; j++) {
				if(i==j || i+j==6) {
					arr[i][j] = '*';
				}else {
					arr[i][j] = ' ';
				}
				System.out.println(arr[i][j]);
			}
			
		}
		
		
		
	}

}
