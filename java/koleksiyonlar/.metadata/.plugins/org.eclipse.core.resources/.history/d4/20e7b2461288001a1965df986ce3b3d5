import java.util.ArrayList;

public class TypeSafe {

	public static void main(String[] args) {

		ArrayList <String> sehir = new ArrayList<String>();
		sehir.add("Ä°stanbul");
		sehir.add("Izmir");
		sehir.add("Ankara");
		
		sehir.remove(0);
		
		for(String s:sehir) {
			System.out.println(s);
		}
		
		
	
	}

}
